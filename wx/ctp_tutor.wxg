                                    <cols>2</cols>
                                    <vgap>0</vgap>
                                    <object class="sizeritem">
                                        <flag>wxALL</flag>
                                        <border>7</border>
                                        <option>0</option>
                                        <object class="wxTextCtrl" name="text_ctrl_5_copy" base="EditTextCtrl">
                                            <value>ctp_run -e izindo -o options.xml -f state.sql</value>
